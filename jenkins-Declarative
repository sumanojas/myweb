pipeline{
    agent any
    
    environment{
        PATH = "/opt/maven3/bin:$PATH"
        }
        
    stages{
        stage("Git checkout"){
            steps{
                git credentialsId: 'github', url: 'https://github.com/javahometech/myweb'
            }
        }
        stage("Maven Build"){
        steps{
        sh "mvn clean package"
        }
        }
    }
}
